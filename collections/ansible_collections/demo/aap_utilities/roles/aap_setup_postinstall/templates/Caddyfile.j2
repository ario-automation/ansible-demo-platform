{{ controller_main_url | regex_replace('https?://', '') }} {
    tls internal

    reverse_proxy http://127.0.0.1:8080
}

{{ hub_main_url | regex_replace('https?://', '') }} {
    tls internal

    reverse_proxy http://127.0.0.1:8081
}

{{ eda_main_url | regex_replace('https?://', '') }} {
    tls internal

    handle {
        reverse_proxy http://127.0.0.1:8082
    }
}

{{ ansible_host }} {
    tls internal

    respond "" 200
}
